  <owl:Class rdf:about="https://vocab.methodandstructure.com/ibis#Argument">
    <rdfs:comment xml:lang="en">An Argument is a type of Issue that explicitly supports or refutes a Position.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/ibis#"/>
    <rdfs:label xml:lang="en">Argument</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://vocab.methodandstructure.com/ibis#Issue"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="https://vocab.methodandstructure.com/ibis#Argument"/>
        <owl:onProperty rdf:resource="https://vocab.methodandstructure.com/ibis#replaces"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="https://vocab.methodandstructure.com/ibis#Argument"/>
        <owl:onProperty rdf:resource="https://vocab.methodandstructure.com/ibis#replaced-by"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="https://vocab.methodandstructure.com/ibis#Position"/>
    <skos:usageNote xml:lang="en">An Argument need not only relate in scope to another Argument, but it must only be replaced by another argument.</skos:usageNote>
  </owl:Class>
  <owl:Class rdf:about="https://vocab.methodandstructure.com/ibis#Entity">
    <rdfs:comment xml:lang="en">ibis:Entity is the abstract superclass of from which the more specific entities are derived.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/ibis#"/>
