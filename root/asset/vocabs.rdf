    <owl:versionInfo>0.1</owl:versionInfo>
    <sh:namespace rdf:resource="https://vocab.methodandstructure.com/graph-tool#"/>
    <sh:prefix rdf:datatype="http://www.w3.org/2001/XMLSchema#token">cgto</sh:prefix>
  </owl:Ontology>
  <owl:Class rdf:about="https://vocab.methodandstructure.com/graph-tool#Error">
    <rdfs:comment xml:lang="en">An error is a type of cgto:View the tool can send the user when something is wrong. It is more suitable to send as a response body than http:Response, which would be problematically self-referential.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/graph-tool#"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#token">Error</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://vocab.methodandstructure.com/graph-tool#View"/>
  </owl:Class>
  <owl:Class rdf:about="https://vocab.methodandstructure.com/graph-tool#Index">
    <rdfs:comment xml:lang="en">An index relates a cgto:Space to a set of cgto:Inventory resources, via a set of cgto:Summary resources.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/graph-tool#"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#token">Index</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://rdfs.org/sioc/ns#Container"/>
  </owl:Class>
  <owl:Class rdf:about="https://vocab.methodandstructure.com/graph-tool#Inventory">
    <rdfs:comment xml:lang="en">An inventory is a de facto grouping of resources according to some criterion.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/graph-tool#"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#token">Inventory</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://rdfs.org/sioc/ns#Container"/>
  </owl:Class>
  <owl:Class rdf:about="https://vocab.methodandstructure.com/graph-tool#Space">
    <rdfs:comment xml:lang="en">A graph tool needs a shared logical space to lay out its universe of discourse.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/graph-tool#"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#token">Space</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://rdfs.org/sioc/ns#Space"/>
  </owl:Class>
  <owl:Class rdf:about="https://vocab.methodandstructure.com/graph-tool#Summary">
    <rdfs:comment xml:lang="en">A summary indexes the available cgto:Inventory resources.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/graph-tool#"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#token">Summary</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/linked-data/cube#DataSet"/>
  </owl:Class>
  <owl:Class rdf:about="https://vocab.methodandstructure.com/graph-tool#View">
    <rdfs:comment xml:lang="en">A partially or fully computed geometric rendering of a (sub)graph, or the structured information needed to do so.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/graph-tool#"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#token">View</rdfs:label>
  </owl:Class>
  <qb:AttributeProperty rdf:about="https://vocab.methodandstructure.com/graph-tool#asserted-domain">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en"></rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/linked-data/cube#Observation"/>
    <rdfs:isDefinedBy rdf:resource="https://vocab.methodandstructure.com/graph-tool#"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#token">asserted-domain</rdfs:label>
    <rdfs:range rdf:resource="https://vocab.methodandstructure.com/graph-tool#Inventory"/>
  </qb:AttributeProperty>
  <qb:MeasureProperty rdf:about="https://vocab.methodandstructure.com/graph-tool#asserted-domain-count">
